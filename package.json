{
	"name": "use-s3-storage-example",
	"version": "1.0.0",
	"description": "An example repo of different ways in using S3 storage",
	"main": "dist/index.js",
	"scripts": {
		"start": "NODE_ENV=production node dist/index.js",
		"dev": "NODE_ENV=development ts-node-dev --respawn src/index.ts",
		"build:kill-dist": "rimraf ./dist",
		"build:code": "tsc",
		"build": "npm run build:kill-dist && npm run build:code",
		"postinstall": "prisma generate",
		"migrate": "prisma migrate dev --name init",
		"seed": "ts-node prisma/seed.ts"
	},
	"keywords": [],
	"author": "SeanCassiere",
	"license": "ISC",
	"devDependencies": {
		"@types/cookie-parser": "^1.4.3",
		"@types/express": "^4.17.13",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.0.6",
		"prisma": "^4.1.0",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@prisma/client": "^4.1.0",
		"aws-sdk": "^2.1179.0",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.0.1",
		"ejs": "^3.1.8",
		"express": "^4.18.1",
		"jsonwebtoken": "^8.5.1",
		"multer": "^1.4.5-lts.1",
		"nanoid": "^4.0.0",
		"zod": "^3.17.9"
	}
}
